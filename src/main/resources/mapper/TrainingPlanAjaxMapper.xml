<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
	namespace="com.gym.GYM.trainingplan.dao.TrainingPlanAjaxDAO">

	<select id="beginner" resultType="training">
		SELECT DISTINCT TRAININGPART FROM TRAINING WHERE TRAININGGRADE = '초급'
	</select>

	<select id="expert" resultType="training">
		SELECT * FROM TRAINING WHERE TRAININGGRADE = '중급'
	</select>


	<select id="sendPart" parameterType="String" resultType="training">
		SELECT * FROM TRAINING WHERE TRAININGPART = #{trainingPart}
	</select>
</mapper>